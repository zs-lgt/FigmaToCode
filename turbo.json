{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:watch": {
      "outputs": [".next/**", "!.next/cache/**"],
      "persistent": true,
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type-check": {}
  }
}
